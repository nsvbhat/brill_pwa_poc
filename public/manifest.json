{
  "id": "/ambetter-health-pwa",
  "lang": "en-US",
  "name": "Ambetter Health PWA",
  "short_name": "Ambetter",
  "description": "Your health, simplified. Access health information 24/7.",
  "version": "1.0.2",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["standalone", "window-controls-overlay"],
  "orientation": "portrait-primary",
  "theme_color": "#ec4899",
  "background_color": "#ffffff",
  "prefer_related_applications": false,
  "categories": ["health", "medical"],
  "screenshots": [
    {
      "src": "/ambetter-logo-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "form_factor": "narrow"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/pdf": [".pdf"],
        "application/msword": [".doc"],
        "application/vnd.openxmlformats-officedocument.wordprocessingml.document": [".docx"],
        "application/vnd.ms-excel": [".xls"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"],
        "text/plain": [".txt"],
        "image/jpeg": [".jpg", ".jpeg"],
        "image/png": [".png"]
      },
      "launch_type": "single-client"
    }
  ],
  "permissions": [
    "camera",
    "microphone",
    "geolocation"
  ],
  "icons": [
    {
      "src": "/ambetter-logo-new.png",
      "sizes": "80x80",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/ambetter-logo-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/ambetter-logo-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    }
  ]
}
